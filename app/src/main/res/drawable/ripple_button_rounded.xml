<?xml version="1.0" encoding="utf-8"?>
<ripple android:color="@color/variant3" xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/mask">
        <shape android:shape="rectangle">
            <corners android:radius="10dp" />
            <solid android:color="@color/variant3"/>
        </shape>
    </item>
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <gradient
                android:angle="0"
                android:startColor="@color/variant4"
                android:endColor="@color/variant5"
                android:type="linear"
                />
            <corners android:radius="10dp" />
        </shape>
    </item>
</ripple>